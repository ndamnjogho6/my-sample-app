{
  "family": "my-sample-app-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::628838486061:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "my-sample-app",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
