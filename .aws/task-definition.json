{
  "family": "my-sample-app-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::628838486061:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "my-sample-app",
      "image": "<IMAGE_URI>",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "MY_SECRET_VALUE",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:628838486061:secret:my-sample-app/production"
        }
      ],
      ...
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
